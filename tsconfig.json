{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "bundler",
      "types": ["node"],
      "jsx": "react-jsx",
      "jsxImportSource": "@builder.io/qwik",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "outDir": "dist"
    },
    "include": ["src", "scripts"]
  }