{
  "name": "treeview",
  "version": "0.0.6",
  "description": "a tree view ui for hierarchical data",
  "main": "index.js",
  "scripts": {
    "qwik": "qwik",
    "dev": "vite --mode ssr",
    "build": "npm run build.types && npm run build.client && npm run build.server",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx --outDir ./dist --mode ssr",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "preview": "qwik build preview && vite preview --open",
    "preview:pwa": "npx serve dist -l 4173",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cleanup": "npx tsx src/test/cleanup.ts",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:cleanup": "npx tsx cypress/support/cleanup-firestore.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blarvin/complete-maintenance-management.git"
  },
  "keywords": [
    "tree"
  ],
  "author": "blarvin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blarvin/complete-maintenance-management/issues"
  },
  "homepage": "https://github.com/blarvin/complete-maintenance-management#readme",
  "dependencies": {
    "@builder.io/qwik": "^1.16.0",
    "dexie": "^4.2.1",
    "firebase": "^12.1.0"
  },
  "devDependencies": {
    "@builder.io/qwik-city": "^1.18.0",
    "@types/node": "^24.3.0",
    "cypress": "^15.8.1",
    "eslint": "^9.34.0",
    "fake-indexeddb": "^6.2.5",
    "globals": "^17.0.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.53.0",
    "vite": "^6.4.1",
    "vitest": "^4.0.14"
  }
}
